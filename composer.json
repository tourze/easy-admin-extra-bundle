{
  "name": "tourze/easy-admin-extra-bundle",
  "description": "EasyAdmin扩展",
  "type": "symfony-bundle",
  "license": "MIT",
  "require": {
    "php": "^8.1",
    "doctrine/dbal": "^3.7.0 || ^4.0",
    "doctrine/persistence": "^3.1 || ^4",
    "doctrine/orm": "^2.17 || ^3.0",
    "symfony/config": "^6.4",
    "symfony/cache-contracts": "^3",
    "symfony/dependency-injection": "^6.4",
    "symfony/http-kernel": "^6.4",
    "symfony/framework-bundle": "^6.4",
    "symfony/serializer": "^6.4",
    "psr/log": "^3|^2|^1",
    "phpdocumentor/reflection-docblock": "^5.4.0",
    "yiisoft/strings": "^2.1",
    "tourze/easy-admin-attribute": "0.0.*",
    "tourze/enum-extra": "0.0.*"
  },
  "autoload": {
    "psr-4": {
      "Tourze\\EasyAdminExtraBundle\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tourze\\EasyAdminExtraBundle\\Tests\\": "tests"
    }
  },
  "require-dev": {
    "phpstan/phpstan": "^2.1",
    "phpunit/phpunit": "^10.0",
    "symfony/browser-kit": "^6.4",
    "symfony/phpunit-bridge": "^6.4"
  },
  "scripts": {
    "phpstan": "phpstan analyse src -l 1",
    "test": "phpunit --bootstrap=tests/bootstrap.php"
  }
}