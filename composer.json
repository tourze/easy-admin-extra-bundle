{
  "name": "tourze/easy-admin-extra-bundle",
  "description": "EasyAdmin扩展",
  "type": "symfony-bundle",
  "license": "MIT",
  "require": {
    "doctrine/dbal": "^4.0",
    "doctrine/orm": "^3.0",
    "doctrine/persistence": "^4.1",
    "easycorp/easyadmin-bundle": "^4",
    "phpdocumentor/reflection-docblock": "^5.4.0",
    "psr/container": "^1.1|^2.0",
    "psr/log": "^3|^2|^1",
    "symfony/cache-contracts": "^3",
    "symfony/config": "^7.3",
    "symfony/dependency-injection": "^7.3",
    "symfony/event-dispatcher-contracts": "^3",
    "symfony/framework-bundle": "^7.3",
    "symfony/http-foundation": "^7.3",
    "symfony/http-kernel": "^7.3",
    "symfony/serializer": "^7.3",
    "symfony/twig-bundle": "^7.3",
    "symfony/yaml": "^7.3",
    "tourze/bundle-dependency": "1.*",
    "tourze/easy-admin-attribute": "0.1.*",
    "tourze/enum-extra": "0.1.*",
    "tourze/symfony-dependency-service-loader": "0.0.*",
    "tourze/symfony-ecol-bundle": "0.1.*",
    "yiisoft/strings": "^2.1"
  },
  "require-dev": {
    "phpstan/phpstan": "^2.1",
    "phpunit/phpunit": "^11.5",
    "symfony/browser-kit": "^7.3",
    "symfony/phpunit-bridge": "^7.3",
    "tourze/phpunit-base": "1.*",
    "tourze/phpunit-symfony-unit-test": "0.0.*",
    "tourze/symfony-testing-framework": "0.0.*"
  },
  "autoload": {
    "psr-4": {
      "Tourze\\EasyAdminExtraBundle\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tourze\\EasyAdminExtraBundle\\Tests\\": "tests"
    }
  },
  "scripts": {
    "phpstan": "phpstan analyse src -l 1",
    "test": "phpunit --bootstrap=tests/bootstrap.php"
  }
}